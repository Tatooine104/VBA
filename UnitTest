Private sub UnitTest()
' Профиль распределения 
Dim PR() as string
PR = Array("СХ", "ПБЛ", "КД")
PR = Array("Z1", "Z4", "Z5")

txt = "что-то"
' Лог теста
' Указываем путь к лог файлу
' на рабочем столе
' FileName = Environ("HOMEDRIVE") & Environ("HOMEPATH") & "\Desktop\UnitTest Log.txt"
' В папке с файлом
FileName = ActiveWorkbook.Path & "\UnitTest Log.txt"
if Len(Dir(FileName)) = 0 Then
  Set FSO = CreateObject("scripting.filesystemobject")
  Set ts = FSO.CreateTextFile(FileName, True): ts.Close
  Set ts = FSO.OpenTextFile(FileName, 8, True): ts.Write txt: ts.Close
  Set ts = Nothing: Set FSO = Nothing
else
  Set FSO = CreateObject("scripting.filesystemobject")
  Set ts = FSO.OpenTextFile(FileName, 8, True): ts.Write txt: ts.Close
  Set ts = Nothing: Set FSO = Nothing
end if

End sub
